{
  "name": "typescript-eslint-parser-example",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:ismail-syed/typescript-eslint-parser-example.git",
  "author": "Ismail Syed <ismail.syed@shopify.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts --max-warnings 0"
  },
  "devDependencies": {
    "eslint": "^4.7.2",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^8.0.0"
  },
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "rules": {
      "array-callback-return": "error",
      "getter-return": "error",
      "strict": "error",
      "lines-around-directive": "error",
      "no-empty-function": "error"
    }
  }
}
